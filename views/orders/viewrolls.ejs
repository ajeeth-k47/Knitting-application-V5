<div id="viewtab" style="width:75%;">

    <h5>View <%= currentPrgId %> Roll Details</h5>
    <br />
    <br />
   
    <table class="styled-table">

        <thead>
            <tr>
                <th>Seq no</th>
                <th>Program No</th>
                <th>Design No</th>
                <th>Total Weight</th>
                <th>Total Rolls</th>
                <th>Production Date</th>
     
            </tr>
        </thead>
        <tbody>
      <% rolls.forEach(function(roll,index) { %>
       <% const newRollDate = (roll.rollDate).getDate()+"-"+((roll.rollDate).getMonth()+1)+"-"+(roll.rollDate).getFullYear();%>
        <tr>
           <td><%= index+1%></td>
           <td><%= roll.programNo%></td>
           <td><%= roll.designNo%></td>
           <td><%= roll.rollWeight%> Kgs</td>
           <td><%= roll.rollcount%></td>
           <td><%= newRollDate%></td>
        </tr>
      <% }) %>
    </tbody>
    </table>

    
    <div class="col-lg-3">
      <button class="btn btn-info"><a style="text-decoration: none;color:white;" href="/api/v1/order/programlist">Back</a></button>
    </div>

</div>